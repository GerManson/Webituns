body {
	background-image: url("../img/bg.jpg");
	background-color: #000000;
	font-family: Arial, sans-serif;
	font-size: 0.8em;
}

ul#icons {margin: 0; padding: 0;}
ul#icons li {margin: 2px; position: relative; padding: 4px 0; cursor: pointer; float: left;  list-style: none;}
ul#icons span.ui-icon {float: left; margin: 0 4px;}

#header {
	margin: 0 auto;
	width: 970px;
	height: auto;
	background-color: #ffffff;
	padding: 10px;
}

#header span {
	font-size: 5.5em;
	line-height: 20px;
	padding-left: 10px;
	padding-right: 10px;
}

#header .menu {
	float: right;
	margin-right: 20px;
}

.rounded {
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border: 1px solid #828282;
}

#content {
	margin: 15px auto;
	width: 970px;
	height: 400px;
	background-color: #ffffff;
	padding: 10px;	
}

#volume {
	width: 175px;
	height: 10px;
	float: left;
	margin: 10px 10px 10px 0px;
}

#volume_amount {
	background: transparent;
	margin-top: 8px;
	border: 0px;
	color: #f6931f;
	font-weight: bold;
	width: 40px;
	float: left;
}

#player_status {
	margin-top: 8px;
	display: block;
	clear: both;
}

#player_status a {
	color: #f6931f;
	text-decoration: none;
}

#player_status a:hover {
	color: #E8CD17;
}

#player_status div {
	background: transparent;
	border: 0px;
	color: #AAADAD;
	font-size: 0.95em;
}

#player_status img {
	padding-right: 5px;
}

#loading_img {
	float: left;
}

#footer {
	margin: 0 auto;
	width: 970px;
	height: auto;
	background-color: #ffffff;
	padding: 10px;
	text-align: center;
}

.subheader {
	clear: both;
	width: 970px;
	height: 90px;
}

.controls {
	float: left;
}

#queue_list {
	float: right;
	width: 420px;
	padding-left: 50px;
}

#queue_list .left,
#queue_list .right {
	margin: 30px 10px;
	float: left;
}

#queue_list a img {
	border: 0px;
	float: left;
}

#queue_list .scroller {
	width: 330px;
	height: 82px;
	overflow: hidden;
	float: left;
}

#queue_list .scroller_content {
	display: inline-block;
	height: 82px;
}

#queue_list .song {
	float: left;
	padding: 3px;
	border: 3px solid white;
	margin: 1px;
}

#songs {
	color: #3A3A3A;
}

#songs .info_commands {
	width: 310px;
	float: left;
}

#songs .search {
	padding-top: 15px;
	padding-bottom: 15px;
	text-align: center;
	width: 100%;
	background-color: #3f3846;
	color: #fff;
	border: 1px solid #828282;
	border-top: 0px;
}

#songs .search input {
	padding: 3px;
	color: #696969;
}

#songs .list {
	float: right;
	width: 650px;
	height: 320px;
	background: #ffffff;
	overflow: auto;
}

#songs .list ul {
	margin: 0;
	padding-left: 0;
	list-style-type: none;
}

#songs .list a {
	display: block;
	padding: 3px;
	background-color: #E8E8E8;
	border: 1px solid #A3A6A6;
	border-top: 0px;
	color: #3A3A3A;
	padding-left: 5px;
}

#songs .list li {
		width: 99.5%;
}

#songs list li:hover {
	cursor: pointer;
}

#songs .list a:link, #navlist a:visited {
	text-decoration: none;
}

#songs .list a:hover {
	background-color: #CDCDCD;
}


.clearfix:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}
 
.clearfix {
	display: inline-block;
}
 
html[xmlns] .clearfix {
	display: block;
}
 
* html .clearfix {
	height: 1%;
}